<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
	<mime-type type="application/x-tuxguitar">
		<comment>TuxGuitar file</comment>
		<comment xml:lang="de">TuxGuitar-Datei</comment>
		<comment xml:lang="it">File di TuxGuitar</comment>
		<glob pattern="*.tg"/>
		<icon name="tuxguitar"/>
	</mime-type>
	<mime-type type="application/x-gtp">
		<comment>Guitar Pro file</comment>
		<comment xml:lang="de">Guitar-Pro-Datei</comment>
		<comment xml:lang="it">File di Guitar Pro</comment>
		<glob pattern="*.gtp"/>
		<glob pattern="*.gp3"/>
		<glob pattern="*.gp4"/>
		<glob pattern="*.gp5"/>
		<glob pattern="*.gp"/>
		<icon name="tuxguitar"/>
	</mime-type>
	<mime-type type="application/x-gtp">
		<comment>Guitar Pro file</comment>
		<comment xml:lang="de">Guitar-Pro-Datei</comment>
		<comment xml:lang="it">File di Guitar Pro</comment>
		<!-- The file extension gpx is also used for GPX geographic data. Guitar Pro gpx files always begin with "BCFZ" -->
		<magic priority="50">
			<match value="BCFZ" type="string" offset="0" />
		</magic>
		<glob pattern="*.gpx"/>
		<icon name="tuxguitar"/>
	</mime-type>
	<mime-type type="application/x-ptb">
		<comment>PowerTab file</comment>
		<comment xml:lang="de">PowerTab-Datei</comment>
		<comment xml:lang="it">File di PowerTab</comment>
		<glob pattern="*.ptb"/>
		<icon name="tuxguitar"/>
	</mime-type>
	<mime-type type="application/x-tef">
		<comment>TablEdit file</comment>
		<comment xml:lang="de">TablEdit-Datei</comment>
		<comment xml:lang="it">File di TablEdit</comment>
		<glob pattern="*.tef"/>
		<icon name="tuxguitar"/>
	</mime-type>
</mime-info>
